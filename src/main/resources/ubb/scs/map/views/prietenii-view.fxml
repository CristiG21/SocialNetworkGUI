<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="520.0" prefWidth="800.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="ubb.scs.map.controller.PrieteniiController">
    <children>
        <SplitPane dividerPositions="0.5" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="521.0">
                    <children>
                        <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0"
                              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <children>
                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="18.0" prefWidth="1010.0"
                                       text="Received Friendships" textAlignment="CENTER">
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </Label>
                                <TableView fx:id="tableViewReceived" prefHeight="500.0" prefWidth="663.0">
                                    <columns>
                                        <TableColumn fx:id="tableColumnReceivedFirstName" prefWidth="331.5"
                                                     text="First Name"/>
                                        <TableColumn fx:id="tableColumnReceivedLastName" minWidth="0.0"
                                                     prefWidth="329.5" text="Last Name"/>
                                        <TableColumn fx:id="tableColumnReceivedFriendsFrom" minWidth="0.0"
                                                     prefWidth="329.5" text="Friends From"/>
                                        <TableColumn fx:id="tableColumnReceivedAccepted" minWidth="0.0"
                                                     prefWidth="329.5" text="Accepted"/>
                                    </columns>
                                    <columnResizePolicy>
                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                    </columnResizePolicy>
                                </TableView>
                                <HBox prefHeight="34.0" prefWidth="655.0" spacing="20.0">
                                    <children>
                                        <Button mnemonicParsing="false" onAction="#handleDeleteReceivedPrietenie"
                                                text="Delete"/>
                                        <Button mnemonicParsing="false" onAction="#handleAcceptPrietenie"
                                                text="Accept"/>
                                    </children>
                                    <padding>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </padding>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </AnchorPane>
                <AnchorPane prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0"
                              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <children>
                                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="18.0" prefWidth="1010.0"
                                       text="Sent Friendships" textAlignment="CENTER">
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </Label>
                                <TableView fx:id="tableViewSent" prefHeight="500.0" prefWidth="663.0">
                                    <columns>
                                        <TableColumn fx:id="tableColumnSentFirstName" prefWidth="331.5"
                                                     text="First Name"/>
                                        <TableColumn fx:id="tableColumnSentLastName" minWidth="0.0" prefWidth="329.5"
                                                     text="Last Name"/>
                                        <TableColumn fx:id="tableColumnSentFriendsFrom" minWidth="0.0" prefWidth="329.5"
                                                     text="Friends From"/>
                                        <TableColumn fx:id="tableColumnSentAccepted" minWidth="0.0" prefWidth="329.5"
                                                     text="Accepted"/>
                                    </columns>
                                    <columnResizePolicy>
                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                    </columnResizePolicy>
                                </TableView>
                                <HBox prefHeight="34.0" prefWidth="655.0" spacing="20.0">
                                    <children>
                                        <Button mnemonicParsing="false" onAction="#handleAddPrietenie" text="Add"/>
                                        <Button mnemonicParsing="false" onAction="#handleDeleteSentPrietenie"
                                                text="Delete"/>
                                    </children>
                                    <padding>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </padding>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </children>

</AnchorPane>
